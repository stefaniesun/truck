<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
<meta http-equiv="pragma" content="no-cache" />
<meta http-equiv="cache-control" content="no-cache" />
<title>新增单品库存</title>
</head>
<body>
<center>
	<form>
		<table cellpadding="4" cellspacing="6" style="margin-top:6px;">
			<tr>
				<td>库存别名</td>
				<td style="text-align:left;">
					<input id="nickNameForm" type="text" style="width:260px;"/>
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">供应商</td>
				<td>
					<input id="providerForm" type="text" style="width:260px;"/>
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">库存类型</td>
				<td>
					<select id="typeForm" class="easyui-combobox" style="width:260px;" data-options="required:true,panelHeight:54">
						<option value="0">现询</option>
						<option value="1" selected="selected">实库</option>
					</select>
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">总库存</td>
				<td>
					<input id="stockForm"  type="text" style="width:260px;" 
					class="easyui-numberbox numberboxForm" 
					data-options="required:true,min:0,max:999999999"/>间
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">可超买数</td>
				<td>
					<input id="surpassForm" type="text" style="width:260px;" 
					 class="easyui-numberbox" value="0"
					 data-options="min:0,max:999999999"/>间
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">成本价</td>
				<td>
					<input id="costForm" type="text" style="width:260px;" 
					class="easyui-numberbox numberboxForm" 
					data-options="required:true,min:0,max:999999,precision:2"/>
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">成本说明</td>
				<td>
					<input id="costRemarkForm" type="text" style="width:260px;"/>
				</td>
			</tr>
			<tr>
				<td style="text-align:right;">有效期</td>
				<td>
					<input class="easyui-datetimebox" id="overDateForm" style="width:260px">
				</td>
			</tr>
			<!-- <tr>
				<td style="text-align:right;">优先级</td>
				<td>
					<input id="priorityForm" type="text" style="width:260px;" 
					class="easyui-numberbox numberboxForm" 
					value="5" data-options="required:true,min:0,max:9" />
				</td>
			</tr> -->
			<tr>
				<td style="text-align:right;">备注</td>
				<td>
					<input id="remarkForm" type="text" style="width:260px;" 
					data-options="validType:'length[0,200]'"/>
				</td>
			</tr>
		</table>
	</form>
</center>
</body>
</html>