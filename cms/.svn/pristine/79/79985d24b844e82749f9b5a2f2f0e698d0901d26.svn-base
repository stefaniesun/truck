package xyz.work.meituan.model;

public class BaseInfo {
    //TaobaoTravelItem表 的信息
    private String travelItem;   //用于关联 travelItem
    
    private String channel;  //渠道编号
    
    private String channelNameCn;   //渠道名称
    
    //TaobaoCruiseItemExt表 的信息
    private String company; //邮轮公司
    
    private String cruiseLine; //邮轮线路
    
    private String shipFeeInclude; //邮轮相关小费
    
    private String shipUp; //邮轮上船地点
    
    private String shipDown; //邮轮下船地点
    
    //TaobaoBaseInfo表 的信息
    private String numberCode; //宝贝编号
    
    private String title; //宝贝标题
    
    private String cruise;  //所属邮轮编号
    
    private String cruiseNameCn;  //所属邮轮名称
    
    private int tripMaxDays; //旅游日程-天数
    
    private int accomNights; //几晚
    
    private String fromLocations; //出发地
    
    private String prov; //宝贝所在地省份(不填默认设置 浙江)
    
    private String city; //宝贝所在城市(不填默认设置 杭州)
    
    private String taobaoDesc; //宝贝描述(不超过50000个字符) 必填
    
    private String toLocations; //目的地
    
    private String subTitles; //宝贝亮点
    
    private String picUrls; //宝贝图片地址
    
    private int itemType; //商品类型

    public String getTravelItem() {
        return travelItem;
    }

    public void setTravelItem(String travelItem) {
        this.travelItem = travelItem;
    }

    public String getChannel() {
        return channel;
    }

    public void setChannel(String channel) {
        this.channel = channel;
    }

    public String getChannelNameCn() {
        return channelNameCn;
    }

    public void setChannelNameCn(String channelNameCn) {
        this.channelNameCn = channelNameCn;
    }

    public String getCompany() {
        return company;
    }

    public void setCompany(String company) {
        this.company = company;
    }

    public String getCruiseLine() {
        return cruiseLine;
    }

    public void setCruiseLine(String cruiseLine) {
        this.cruiseLine = cruiseLine;
    }

    public String getShipFeeInclude() {
        return shipFeeInclude;
    }

    public void setShipFeeInclude(String shipFeeInclude) {
        this.shipFeeInclude = shipFeeInclude;
    }

    public String getShipUp() {
        return shipUp;
    }

    public void setShipUp(String shipUp) {
        this.shipUp = shipUp;
    }

    public String getShipDown() {
        return shipDown;
    }

    public void setShipDown(String shipDown) {
        this.shipDown = shipDown;
    }

    public String getNumberCode() {
        return numberCode;
    }

    public void setNumberCode(String numberCode) {
        this.numberCode = numberCode;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getCruise() {
        return cruise;
    }

    public void setCruise(String cruise) {
        this.cruise = cruise;
    }

    public String getCruiseNameCn() {
        return cruiseNameCn;
    }

    public void setCruiseNameCn(String cruiseNameCn) {
        this.cruiseNameCn = cruiseNameCn;
    }

    public int getTripMaxDays() {
        return tripMaxDays;
    }

    public void setTripMaxDays(int tripMaxDays) {
        this.tripMaxDays = tripMaxDays;
    }

    public int getAccomNights() {
        return accomNights;
    }

    public void setAccomNights(int accomNights) {
        this.accomNights = accomNights;
    }

    public String getFromLocations() {
        return fromLocations;
    }

    public void setFromLocations(String fromLocations) {
        this.fromLocations = fromLocations;
    }

    public String getProv() {
        return prov;
    }

    public void setProv(String prov) {
        this.prov = prov;
    }

    public String getCity() {
        return city;
    }

    public void setCity(String city) {
        this.city = city;
    }

    public String getTaobaoDesc() {
        return taobaoDesc;
    }

    public void setTaobaoDesc(String taobaoDesc) {
        this.taobaoDesc = taobaoDesc;
    }

    public String getToLocations() {
        return toLocations;
    }

    public void setToLocations(String toLocations) {
        this.toLocations = toLocations;
    }

    public String getSubTitles() {
        return subTitles;
    }

    public void setSubTitles(String subTitles) {
        this.subTitles = subTitles;
    }

    public String getPicUrls() {
        return picUrls;
    }

    public void setPicUrls(String picUrls) {
        this.picUrls = picUrls;
    }

    public int getItemType() {
        return itemType;
    }

    public void setItemType(int itemType) {
        this.itemType = itemType;
    }  
  
}